<div class="col-md-4">
  <div class="panel" style="background-color:#CEBBC1" align="center">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>From:</strong>
      <%= @route.from_station %>
    </p>

    <p>
      <strong>To:</strong>
      <%= @route.to_station %>
    </p>

    <p>
      <strong>Distance:</strong>
      <%= @route.distance %>
    </p>

    <%= link_to 'Edit', edit_agency_route_path(current_agency, @route), class: "btn btn-info"%>
    <%= link_to 'Back to Route List', agency_routes_path, class: "btn btn-info" %>
  </div>
</div>
